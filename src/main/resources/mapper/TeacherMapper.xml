<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.score.mapper.TeacherMapper">
    <select id="getMajorByMajorid" resultType="java.lang.String">
        select
        	majorname 
        from 
        	Major 
        where 
        	id = #{majorId};
    </select>
</mapper>
